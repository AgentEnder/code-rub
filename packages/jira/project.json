{
  "root": "packages/jira",
  "sourceRoot": "packages/jira/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/jira/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/jira"
      ],
      "options": {
        "jestConfig": "packages/jira/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": [
          {
            "input": "packages/jira",
            "glob": "**/*.json",
            "ignore": [
              "**/tsconfig*.json",
              "project.json"
            ],
            "output": "/"
          },
          {
            "input": "packages/code-rub",
            "glob": "**/*.md",
            "output": "/"
          }
        ],
        "main": "packages/jira/src/plugin.ts",
        "tsConfig": "packages/jira/tsconfig.lib.json",
        "outputPath": "dist/packages/@code-rub/jira",
        "packageJson": "packages/jira/package.json",
        "generatePackageJson": true
      },
      "outputs": [
        "{options.outputPath}"
      ]
    }
  },
  "tags": []
}
