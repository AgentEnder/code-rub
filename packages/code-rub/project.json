{
  "root": "packages/code-rub",
  "sourceRoot": "packages/code-rub/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/code-rub/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/code-rub"
      ],
      "options": {
        "jestConfig": "packages/code-rub/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": [
          {
            "input": "packages/code-rub",
            "glob": "**/*.json",
            "ignore": [
              "**/tsconfig*.json",
              "project.json"
            ],
            "output": "/"
          },
          {
            "input": "packages/code-rub",
            "glob": "**/*.md",
            "output": "/"
          }
        ],
        "main": "packages/code-rub/src/index.ts",
        "tsConfig": "packages/code-rub/tsconfig.lib.json",
        "outputPath": "dist/packages/code-rub",
        "packageJson": "packages/code-rub/package.json"
      },
      "outputs": [
        "dist/packages/code-rub"
      ]
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "echo 'not npm-publish'"
      }
    }
  },
  "tags": []
}
